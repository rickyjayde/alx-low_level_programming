#include "main.h"
#include <stdio.h>
#include <ctype.h>

/**
 * print_numbers - prints the numbers from 0 to 9
 * followed by a dollar sign and a new line
 */
void print_numbers(void)
{
    int i;

    for (i = 0; i < 10; i++)
    {
        putchar(i + '0');
    }

    putchar('$'); /* Print the dollar sign */
    putchar('\n'); /* Print a new line */
}

/**
 * main - Entry point of the program
 *
 * Return: Always 0
 */
int main(void)
{
    print_numbers(); /* Call the print_numbers function */
    return 0;
}

